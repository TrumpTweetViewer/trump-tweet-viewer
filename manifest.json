{
  "name" : "Trump Tweet Viewer",
  "version" : "1.0",
  "manifest_version": 2,
  "description" : "View Trump's deleted tweets",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png"
  },
  "permissions": [
   "activeTab", "tabs", "<all_urls>"
   ]
}